{
  "name": "assistant-client",
  "version": "0.1.0",
  "repository": "Invis1ble/assistant-client",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" \"npm run sassc:w\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "sassc:w": "sass --no-cache --watch styles.scss:styles.css app:app",
    "typings": "typings"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.0.2",
    "@angular/compiler": "~2.0.2",
    "@angular/core": "~2.0.2",
    "@angular/forms": "~2.0.2",
    "@angular/http": "~2.0.2",
    "@angular/material": "^2.0.0-alpha.9-3",
    "@angular/platform-browser": "~2.0.2",
    "@angular/platform-browser-dynamic": "~2.0.2",
    "@angular/router": "~3.0.2",
    "@angular/upgrade": "~2.0.2",
    "angular2-jwt": "0.1.23",
    "core-js": "^2.4.1",
    "mdi": "^1.7.22",
    "moment": "^2.13.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "concurrently": "^3.0.0",
    "lite-server": "^2.2.2",
    "sass": "^0.5.0",
    "typescript": "^2.0.3",
    "typings": "^1.4.0"
  }
}
